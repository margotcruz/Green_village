{% extends 'base.html.twig' %}

{% block title %}Adresse{% endblock %}

{% block body %}
    <h1>Adresse</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ adresse.id }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ adresse.adresse }}</td>
            </tr>
            <tr>
                <th>AdresseComplementaire</th>
                <td>{{ adresse.adresseComplementaire }}</td>
            </tr>
            <tr>
                <th>CodePostal</th>
                <td>{{ adresse.codePostal }}</td>
            </tr>
            <tr>
                <th>Ville</th>
                <td>{{ adresse.ville }}</td>
            </tr>
            <tr>
                <th>Statut</th>
                <td>{{ adresse.statut ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>TelephoneSupplementaire</th>
                <td>{{ adresse.telephoneSupplementaire }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_adresse_index') }}">back to list</a>

    <a href="{{ path('app_adresse_edit', {'id': adresse.id}) }}">edit</a>

    {{ include('adresse/_delete_form.html.twig') }}
{% endblock %}
